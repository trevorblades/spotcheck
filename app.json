{
  "expo": {
    "name": "spotcheck",
    "slug": "spotcheck",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trevorblades.spotcheck",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.216378207213-2iarkd2v0qq8g64sf08cl0mf3jkebnq3"
        }
      }
    },
    "android": {
      "package": "com.trevorblades.spotcheck",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
